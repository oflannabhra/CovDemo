<h1>Register</h1>
<%= render "shared/error_messages", :target => @user %>

<%= form_for @user do |user_form| %>
  <p>
  <%= user_form.label :name %><br />
  <%= user_form.text_field :name %>
</p>
<p>
  <%= user_form.label :email %><br />
  <%= user_form.text_field :email %>
</p>

  <%= user_form.fields_for :group do |group_form| %>
    Group <%= group_form.label :name %><br/ >
    <%= group_form.text_field :name %>
  <% end %>

<p>
  <%= user_form.label :password %><br />
  <%= user_form.password_field :password %>
</p>
<p>
  <%= user_form.label :password_confirmation %><br />
  <%= user_form.password_field :password_confirmation %>
</p>



  <%= user_form.submit "Register" %>
<% end %>